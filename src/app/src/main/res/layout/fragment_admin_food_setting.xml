<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.AdminFoodSetting">

    <TextView
        android:id="@+id/AdminFoodSettingTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Món ăn"
        android:textSize="34dp"
        android:textStyle="bold"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/AdminFoodSettingTitle"
        android:id="@+id/updateInformation"
        android:text="Thêm món ăn"
        android:layout_margin="20dp"
        android:textSize="26dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/updateInformation"
        android:id="@+id/history"
        android:text="Danh sách món ăn đề xuất"
        android:layout_margin="20dp"
        android:textSize="26dp" />
</RelativeLayout>